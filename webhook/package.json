  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@assistant/conversation": "^3.12.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
