{
  "name": "car-voice-webhook",
  "version": "1.0.0",
  "description": "Google Assistant webhook for car voice control",
  "main": "webhook.js",
  "scripts": {
    "start": "node webhook.js",
    "dev": "nodemon webhook.js",
    "deploy": "gcloud functions deploy carVoiceWebhook --runtime nodejs18 --trigger-http --allow-unauthenticated"
